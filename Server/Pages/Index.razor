@page "/"
@using Application.Requests;
@inject TradeCommand tradeCommand



<TradingViewWidget />

<MudButton Variant="Variant.Filled" Color="Color.Primary" @onclick="ClickTest">Primary</MudButton>

@code{
    private async void ClickTest()
    {
        var tradeRequest = new TradeRequest();
        tradeRequest.Trade = new Domain.Entities.Trade() { AccountId = 1, Attempt = 0, Id = 6, LateEntry = 1, PercentageEntry = false, Symbol = "MODIFIED"};
        tradeRequest.TradeBot = Domain.Entities.TradeBot.FakeBreakout;
        await tradeCommand.RegisterTrade(tradeRequest);
    }
}